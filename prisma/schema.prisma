generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model CoffeeShop {
	id Int @id @default(autoincrement())
	name String
	address String
	description String @default("default")
	imgUrl String
	quiet Int
	wifi Int
	outlets Int
	coffee Int
	tea Int
}