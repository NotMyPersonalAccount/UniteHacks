// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model CoffeeShop {
	name String @unique
	address String @unique
	imgUrl String @unique
	quiet Int
	wifi Int
	outlets Int
	coffee Int
	tea Int
}